{
  "name": "vardot/uber_publisher-project",
  "description": "Project template for Uber Publisher distribution.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Vardot",
      "homepage": "https://github.com/vardot",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/uber_publisher",
    "source": "http://cgit.drupalcode.org/uber_publisher"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~1.0",
    "oomphinc/composer-installers-extender": "~1.0",
    "cweagans/composer-patches": "~1.0",
    "drupal-composer/drupal-scaffold": "~2.0",
    "drupal/drupal-library-installer-plugin": "^0.3",
    "webflo/drupal-finder": "^1.1.0",
    "webmozart/path-util": "^2.3.0",
    "vardot/varbase": "~8.6.0",
    "drupal/linked_field": "^1.1",
    "drupal/sharerich": "^1.1",
    "drupal/blocktabs": "^1.0-alpha6",
    "drupal/social_post_twitter": "^2.0-alpha2",
    "drupal/views_exclude_previous": "^1.0.0-rc1",
    "drupal/fb_instant_articles": "^2.1",
    "drupal/ads_system": "^1.0-alpha1",
    "drupal/simple_amp": "^1.15",
    "drupal/subpathauto": "^1.0-beta1",
    "drupal/facets": "^1.0",
    "lullabot/amp": "^1.1",
    "drupal/uber_publisher_core": "6.x-dev",
    "drupal/uber_publisher_author_profile": "6.x-dev",
    "drupal/uber_publisher_opinion_articles": "6.x-dev",
    "drupal/uber_publisher_ads_system": "6.x-dev",
    "drupal/uber_publisher_breaking_news": "6.x-dev",
    "drupal/uber_publisher_daily_cartoons": "6.x-dev",
    "drupal/uber_publisher_in_pictures": "6.x-dev",
    "drupal/uber_publisher_search": "6.x-dev",
    "drupal/uber_publisher_social_post": "6.x-dev"
  },
  "require-dev": {
    "drupal/console": "~1.0",
    "drush/drush": "~9.0",
    "drupal/coder": "~8.0",
    "drupal/drupal-extension": "~3.0",
    "phing/phing": "~2.0",
    "behat/behat": "~3.0",
    "behat/mink": "~1.0",
    "behat/mink-extension": "~2.0",
    "behat/mink-goutte-driver": "~1.0",
    "behat/mink-zombie-driver": "~1.0",
    "behat/mink-selenium2-driver": "~1.0",
    "behatch/contexts": "~3.0",
    "emuse/behat-html-formatter": "^0.1.0"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "UberPublisher\\composer\\": "src/composer"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "Varbase\\composer\\ScriptHandler::createRequiredFiles",
      "Varbase\\composer\\ScriptHandler::removeGitDirectories",
      "@composer drupal-scaffold"
    ],
    "post-update-cmd": [
      "Varbase\\composer\\ScriptHandler::createRequiredFiles",
      "Varbase\\composer\\ScriptHandler::removeGitDirectories"
    ],
    "post-drupal-scaffold-cmd": [
      "Varbase\\composer\\ScriptHandler::postDrupalScaffoldProcedure",
      "UberPublisher\\composer\\ScriptHandler::postDrupalScaffoldSubProfileProcedure"
    ]
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-6.x": "8.6.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": ["drupal/core"],
      "docroot/profiles/{$name}": ["type:drupal-profile"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "docroot/libraries/slick": ["npm-asset/slick-carousel"],
      "docroot/libraries/ace": ["npm-asset/ace-builds"],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "drupal-libraries": {
      "library-directory": "docroot/libraries",
      "libraries": [
          {"name": "dropzone","package": "npm-asset/dropzone"},
          {"name": "blazy", "package": "npm-asset/blazy"},
          {"name": "slick", "package": "npm-asset/slick-carousel"},
          {"name": "ace", "package": "npm-asset/ace-builds"}
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
       "drupal/core": "-p2"
    },
    "patches": {
      "drupal/fb_instant_articles": {
        "Issue #2912129: Fix embed YouTube, Twitter & Instagram?":
        "https://www.drupal.org/files/issues/twitter_instagram_table_picture_transformer-2912129-7.patch"
      },
      "drupal/facets": {
        "Issue #2692027: Have All link in facets":
        "https://www.drupal.org/files/issues/2018-06-09/2692027-78.patch"
      }
    }
  }
}
